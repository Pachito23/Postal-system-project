<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerLogin">
    <children>
        <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#gotoMainMenu" text="Back to Main Menu" />
        <Label layoutX="256.0" layoutY="46.0" text="Log in">
            <font>
                <Font size="32.0" />
            </font>
        </Label>
        <TextField fx:id="usernameField" layoutX="226.0" layoutY="179.0" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="228.0" promptText="Password" />
        <Button layoutX="275.0" layoutY="286.0" mnemonicParsing="false" onAction="#login" text="Log in" />
        <Label fx:id="message" layoutX="188.0" layoutY="138.0" prefHeight="17.0" prefWidth="224.0" textAlignment="CENTER" />
    </children>
</AnchorPane>
