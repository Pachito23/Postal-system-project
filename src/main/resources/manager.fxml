<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerManager">
   <children>
      <Button layoutX="32.0" layoutY="68.0" mnemonicParsing="false" onAction="#seeAllParcels" text="View all parcels" />
      <Label fx:id="parcelsMessage" layoutX="32.0" layoutY="29.0" prefHeight="27.0" prefWidth="506.0" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="144.0" layoutY="349.0" mnemonicParsing="false" onAction="#logOutToMainMenu" text="Log out" />
      <Label fx:id="loggedInAsLabel" layoutX="49.0" layoutY="117.0" prefHeight="216.0" prefWidth="248.0" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="usernameRegisterField" layoutX="389.0" layoutY="103.0" promptText="Username" />
      <PasswordField fx:id="passwordRegisterField" layoutX="388.0" layoutY="141.0" promptText="Password" />
      <TextField fx:id="RegisterField1" layoutX="388.0" layoutY="173.0" />
      <TextField fx:id="RegisterField2" layoutX="388.0" layoutY="208.0" />
      <TextField fx:id="RegisterField3" layoutX="388.0" layoutY="243.0" />
      <Button layoutX="410.0" layoutY="349.0" mnemonicParsing="false" onAction="#registerAccount" text="Register Account" />
      <Label fx:id="message" layoutX="388.0" layoutY="282.0" prefHeight="52.0" prefWidth="149.0" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="173.0" layoutY="68.0" mnemonicParsing="false" onAction="#manageUnassignedParcels" text="Manage unassigned parcels" />
      <ComboBox fx:id="accountOption" layoutX="388.0" layoutY="68.0" onAction="#setPrompts" prefWidth="150.0" />
   </children>
</AnchorPane>
